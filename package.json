{
    "name": "phonegap",
    "description": "PhoneGap command-line interface and node.js library.",
    "version": "0.7.0",
    "homepage": "http://github.com/mwbrooks/phonegap-cli",
    "repository": {
        "type": "git",
        "url": "git://github.com/mwbrooks/phonegap-cli.git"
    },
    "keywords": [
        "cli",
        "cordova",
        "phonegap",
        "phonegap build",
        "phonegap/build"
    ],
    "preferGlobal": "true",
    "main": "./lib/main.js",
    "bin": {
        "phonegap": "./bin/phonegap.js"
    },
    "scripts": {
        "test": "./node_modules/.bin/jasmine-node --color --verbose spec"
    },
    "engines": {
        "node": ">=0.8.0"
    },
    "dependencies": {
        "colors": "0.6.x",
        "cordova": "2.5.x",
        "node-static": "0.6.x",
        "optimist": "0.3.x",
        "phonegap-build": "0.5.x",
        "prompt": "0.2.x",
        "qrcode-terminal": "0.9.x",
        "semver": "1.1.0",
        "shelljs": "0.0.x"
    },
    "devDependencies": {
        "jasmine-node": "1.1.x"
    },
    "optionalDependencies": {
    },
    "author": {
        "name": "Michael Brooks",
        "email": "michael@michaelbrooks.ca",
        "url": "http://michaelbrooks.ca/"
    },
    "contributors": [
    ]
